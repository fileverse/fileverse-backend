{
  "name": "fileverse",
  "version": "1.0.0",
  "description": "API server for fileverse",
  "main": "src/server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha $(find tests -name '*.spec.js') --recursive --exit",
    "test:unit": "NODE_ENV=test mocha $(find tests/unit -name '*.spec.js') --recursive --exit",
    "test:e2e": "NODE_ENV=test mocha $(find tests/e2e -name '*.spec.js') --recursive --exit",
    "start": "NODE_ENV=production node src/server.js",
    "start:cron": "NODE_ENV=production node src/cron.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix",
    "predeploy": "NODE_ENV=production npm install",
    "deploy": "NODE_ENV=production npm start",
    "dev": "nodemon src/server.js",
    "dev:cron": "nodemon src/cron.js"
  },
  "keywords": [
    "rest",
    "express",
    "node",
    "mongodb"
  ],
  "author": "Vijay Krishnavanshi",
  "license": "ISC",
  "dependencies": {
    "@pinata/sdk": "^1.1.23",
    "agenda": "^4.2.1",
    "agendash": "^3.1.0",
    "aws-sdk": "^2.1069.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "big-integer": "^1.6.51",
    "big.js": "^6.1.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.6.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "express-fileupload": "^1.4.0",
    "express-validation": "^3.0.8",
    "file-type": "^16.5.3",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.18.1",
    "md5": "^2.3.0",
    "mongoose": "^6.0.14",
    "moralis": "^1.3.1",
    "morgan": "^1.10.0",
    "newrelic": "^8.9.0",
    "node-cache": "^5.1.2",
    "readable-stream-clone": "^0.0.7",
    "request": "^2.88.2",
    "short-uuid": "^4.2.0",
    "stream-to-blob": "^2.0.1",
    "url-parse": "^1.5.10",
    "uuid": "^8.3.2",
    "web3.storage": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "plop": "^3.0.2",
    "prettier": "^2.5.0"
  },
  "engines": {
    "node": "^14.17.0"
  }
}
